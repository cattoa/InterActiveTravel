    <properties>

        <!-- General configuration properties -->

        <property key="entity" type="entity" required="true" isPath="yes" pathType="reference">
            <caption>Entity</caption>
            <category>Configuration</category>
            <description>The entity to represent Travel</description>
        </property>
        <property key="departureAttr" type="attribute" entityProperty="entity" required="true">
            <caption>Departure attribute</caption>
            <category>Configuration</category>
            <description>The attribute used to describe the departure point.</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="hasFixedDeparture" type="attribute" entityProperty="entity" required="true">
            <caption>Fixed departure</caption>
            <category>Selection</category>
            <description>This determines if the route has a fixed departure time.</description>
            <attributeTypes>
                <attributeType name="Boolean"/>
            </attributeTypes>
        </property>
        
        <property key="departureTimeAttr" type="attribute" entityProperty="entity" required="false">
            <caption>Departure attribute</caption>
            <category>Configuration</category>
            <description>The attribute used as the departure time. Only required when Fixed departure is true.</description>
            <attributeTypes>
                <attributeType name="DateTime"/>
            </attributeTypes>
        </property>
        <property key="destinationAttr" type="attribute" entityProperty="entity" required="true">
            <caption>Destination attribute</caption>
            <category>Configuration</category>
            <description>The attribute used to describe the destination point.</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="arrivalTimeAttr" type="attribute" entityProperty="entity" required="false">
            <caption>Arrival attribute</caption>
            <category>Configuration</category>
            <description>The attribute used as the departure time. Only required when Fixed departure is true.</description>
            <attributeTypes>
                <attributeType name="DateTime"/>
            </attributeTypes>
        </property>
        <property key="travelType" type="attribute" entityProperty="entity" required="true">
            <caption>Travel type</caption>
            <category>Configuration</category>
            <description>The enumerator used as the travel type.</description>
            <attributeTypes>
                <attributeType name="Enum"/>
            </attributeTypes>
        </property>

</widget>